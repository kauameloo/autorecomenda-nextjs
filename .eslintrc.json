{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "react/no-unescaped-entities": "off",
    "@typescript-eslint/no-explicit-any": "off", // Desativa o erro de 'any' inesperado
    "@typescript-eslint/no-unused-vars": "off", // Desativa o erro de variáveis não usadas
    "@typescript-eslint/no-empty-interface": "off", // Desativa o erro de interface vazia
    "@typescript-eslint/no-empty-object-type": "off" // Desativa o erro de objetos vazios
  }
}
